{
    "version": 2,
    "functions": {
        "api/index.php": { 
            "runtime": "vercel-php@2.0.1" 
        }
    },
    "routes": [
        {
            "src": "/(css|js|images|assets|favicon.ico)(.*)",
            "dest": "/public/$1$2"
        },
        {
            "src": "/storage/(.*)",
            "dest": "/public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}